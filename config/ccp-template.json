
{
    "name": "${ORG}",
    "version": "1.0.0",
    "client": {
        "organization": "${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "${ORG}": {
            "mspid": "${MSP}",
            "peers": [
                
    "${PEER0}"
            ,
    "${PEER1}"
            
            ],
            "certificateAuthorities": [
                "${CA}"
            ]
        }
    },
    "peers": {
        
        "${PEER0}": {
            "url": "grpcs://${PEER0}:${P0PORT}",
            "tlsCACerts": {
                "path": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "${PEER0}",
                "hostnameOverride": "${PEER0}"
            }
        }
            ,
        "${PEER1}": {
            "url": "grpcs://${PEER1}:${P1PORT}",
            "tlsCACerts": {
                "path": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "${PEER1}",
                "hostnameOverride": "${PEER1}"
            }
        }
            
    },
    "certificateAuthorities": {
        "${CA}": {
            "url": "https://${CA}:${CAPORT}",
            "tlsCACerts": {
                "path": "${CAPEM}"
            },
            "caName": "${CANAME}",
            "httpOptions": {
                "verify": false
            }
        }
    }
}

        